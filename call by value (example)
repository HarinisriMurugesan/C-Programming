#include <stdio.h>
void swap(int,int);
void main()
{
    int a=10,b=20;
    printf("Before swapping a=%d b=%d \n",a,b);
    swap(a,b);
    printf("after swapping a=%d b=%d",a,b);
}
void swap(int a,int b)
{
    int temp;
    temp=a;
    a=b;
    b=temp;
}

-----------------------------------------------------------------------------------------------------------------------------------------------------------------

OUTPUT :

Before swapping a=10 b=20 
after swapping a=10 b=20

--------------------------------------------------------------------------------------------------------------------------------------------------------------------
