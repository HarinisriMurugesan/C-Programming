#include <stdio.h>
struct student 
{ 
    int id;
    char name[20];
};
void main()
{
    struct student s ,s1;
    printf ("enter a id :");
    scanf ("%d",&s.id);
    scanf ("%d",&s1.id);
    printf ("enter a name :");
    scanf ("%s",&s.name);
    scanf ("%s",&s1.name);
    printf ("%d",s.id);
    printf ("%d",s1.id);
    printf ("%s",s.name);
    printf ("%s",s1.name);
    
}


***********************************************************

OUTPUT :

  enter a id :26
  30

  enter a name :dheepshi harinisri 

  26 30 dheepshi harinisri 

**************************************************************
