#include <stdio.h>
int main()
{
int number;
printf("enter a number : ");
scanf("%d",&number);
switch (number)
{
    case 1:
    printf("january");
    break;
    case 2:
    printf("febuary");
    break;
    case 3:
    printf("march");
    break;
    case 4:
    printf("april");
    break;
    case 5:
    printf("may");
    break;
    default:
    printf("invalid month");
    return 0;
    
}

    return 0;
}

**************************************************************************************************************************************************************

OUTPUT :

enter a number : 5
may

enter a number : 3
march

*****************************************************************************************************************************************************************
