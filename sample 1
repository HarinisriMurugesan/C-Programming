#include <stdio.h>
int main()
{
    float num1;
    double num2;

    printf("Enter a number: ");
    scanf("%f", &num1);
    printf("Enter another number: ");
    scanf("%lf", &num2);

    printf("num1 = %f\n", num1);
    printf("num2 = %lf", num2);

    return 0;
}


------------------------------------------------------------------------------------------------------------------------------------------------------------------------

OUTPUT :

Enter a number: 3.5
Enter another number: 32.8
num1 = 3.500000
num2 = 32.800000

-------------------------------------------------------------------------------------------------------------------------------------------------------------------------
